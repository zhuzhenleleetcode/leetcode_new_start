class Solution {
    public int countPrimes(int n) {
        if(n<=2) return 0;
        int res = n-2;
        boolean[] table = new boolean[n];
        for(int i=2; i*i<n; i++){
            if(table[i])continue;
            for(int j=i*i; j<n; j+=i){
                if(!table[j]){
                    table[j]=true;
                    res--;
                }
            }
        }
        return res;
    }
}
